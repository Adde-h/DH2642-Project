{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [".js", ".ios.js", ".android.js", ".jsx", ".html"],
        "alias": {
          "_screens": "./src/screens",
          "_styles": "./src/styles"
        }
      }
    ]
  ]
}